      <path fill="none" d="M {{354+titlew}}.5 100 l 0 {{tableh}}" stroke="#000" stroke-width="1" opacity="1"></path>
      <path fill="none" d="M {{388+titlew}}.5 100 l 0 {{tableh}}" stroke="#000" stroke-width="1" opacity="1"></path>

    </g>

