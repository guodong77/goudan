<!-- 用户统计  -->
<div class="widget-main biaoge" ng-controller="com_ctrl">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="bg-themeprimary h_10"></div>

        </div>
    </div>
    <div class="tabbable">
        <ul class="nav nav-tabs planmytab">
            <li class="active">
                <a >用户信息</a>
            </li>
        </ul>

        <div class="tab-content">
            <div id="matter" class="tab-pane in active">
                <table class="table table-striped table-bordered table-hover">
                    <thead class="bordered-palegreen">
                        <tr>
<!--                             <th width="50" style="padding: 6px;">
                                <label yscheckall="matterdata" yskey="ycselected">
                                    <input type="checkbox" class="colored-blue">
                                    <span class="text"></span>
                                </label>
                            </th> -->
                            <th>序号</th>
                            <th>ID</th>
                            <th>用户名</th>                           
                            <th>真实姓名</th>
<!--                             <th>登录次数</th>
                            <th>推广次数</th>
                            <th>推荐人</th> -->
                            <th>联系电话</th>
                            <th>用户类型</th>
<!--                             <th>公司ID</th> -->
                            <th>是否锁定</th>
<!--                             <th>单位名称</th>
                            <th>单位类型</th> -->
                            <th>绑定QQ</th>
                            <th>绑定公众号</th>
                            <th>绑定微信</th>
                            <th>邮箱</th>
                            <th>注册日期</th>                            
                            <th>工程上限</th>
                        </tr>

                    </thead>
                    <tbody>
                        <tr  align="center" ng-class="{true:'yssel',false:''}[value.ycselected==1]" ng-repeat="(key, value) in operate_user_list.datalist">
 <!--                            <td align="center">
                                <div class="checkbox">
                                    <label yscheck="ycselected">
                                        <input type="checkbox" class="colored-blue" ng-checked="value.ycselected==1">
                                        <span class="text"></span>
                                    </label>
                                </div>
                            </td> -->
                            <td ng-bind="$index+1"></td>
                            <td ng-bind="value.uid"></td>
                            <td ng-bind="value.username"></td>                           
                            <td ng-bind="value.realname"></td> 
<!--                             <td ng-bind="value.sex"></td>                           
                            <td ng-bind="value.sex"></td> 
                            <td ng-bind="value.tuijian"></td> -->
                            <td ng-bind="value.tel"></td>
                            <td ><span ng-if="value.usertype==1">系统管理员</span>
                            <span ng-if="value.usertype==2">普通管理员</span>
                            <span ng-if="value.usertype==3">普通用户</span>
                            <span ng-if="value.usertype==4">公司管理员</span></td>
                            <!-- <td ng-bind="value.comid"></td> -->
                            <td><span ng-if="value.lockey==0">否</span><span ng-if="value.lockey==1">锁定</span></td>
<!--                             <td ng-bind="value.workname"></td>
                            <td ng-bind="value.worktype"></td> -->
                            <td ng-bind="value.qqopenid==1?'√':''"></td>
                            <td ng-bind="value.wechatopenid==1?'√':''"></td>
                            <td ng-bind="value.pcweopenid==1?'√':''"></td>
                            <td ng-bind="value.email"></td>
                            <td>
                                 <span  ng-bind="value.add_time"></span>
                            </td>
                            <td ng-bind="value.maxprogram"></td>
                        </tr>
                    </tbody>
                </table>

                <div id='page' class="t_c mt_10"></div>
            </div>
        </div>
    </div>
</div>