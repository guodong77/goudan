<div  class="widget-body win form-horizontal bgc_b" >


  <div class="winbody" w="850px" h="600px" style="overflow-x: hidden;overflow-y:auto; ">
  <div>
      <h3 style="margin-top: 6px; margin-bottom: 10px;" class="t_l" >
      <span ng-bind="program_gantt_worklist.hdata[0].htitle"></span>
     <span class="f_16 fr mt_8 mr_10" ng-bind="program_gantt_worklist.hdata[0].etime"></span>
     <span class="f_16 fr mt_8">~</span>
      <span class="f_16 fr mt_8" ng-bind="program_gantt_worklist.hdata[0].btime"></span>
      </h3>
      
  </div>
		  <div class="form-group biaoge" style="margin:0px 0px;max-height: 426px;overflow: auto;">
		    <table class="table table-striped table-bordered table-hover">
		       <thead>
                <tr>
                    <th scope="col" width="30">
                      <label>
                        <input class="colored-blue" comcheckall="program_gantt_worklist" type="checkbox">
                        <span class="text"></span>
                       </label>
                    </th>
                     <th width="60" class="t_c">排序</th>
                    <th  class="t_c">作业名称</th>	
                   
                    <th width="150" class="t_c">开始日期</th>	
                    <th  width="150" class="t_c">结束日期</th>	
                    <th  width="100" class="t_c">工期（天）</th>	
                </tr>
            </thead>
            <tbody>
            <tr  ng-if="value.isdel!=1" ng-class="{true:'yssel',false:''}[value.check==1]"  ng-repeat="(key, value) in program_gantt_worklist.datalist|orderBy:'myorder'">
            <td align="center">
               <div class="checkbox">
                    <label yscheck="byselected" style="  min-height: 26px;">
                     <input class="colored-blue" ng-checked="value.check==1" comcheck="check" type="checkbox">
                        <span class="text"></span>
                    </label>
                </div>

            </td>
              <td>
                <input value="{{value.myorder}}"  comedit='myorder' class="form-control b_0" type="text">
            </td>
             <td>
                <input value="{{value.rtitle}}"  comedit='rtitle' class="form-control b_0" type="text">
            </td>
            <td>
                <input   value="{{value.rbtime}}" duibilaydate='rbtime'  class="form-control" type="text"><!-- laydateedit="rbtime"  -->
            </td>
            <td>
                <input  value="{{value.retime}}" duibilaydate="retime" class="form-control" type="text"><!-- laydateedit="retime"  -->
            </td>
            <td>
                <input value="{{lq(value.rbtime,value.retime)}}" readonly class="form-control" type="text">
            </td>
           </tr>
            </tbody>
		    </table>
	      </div>
	 
		<div class="form-group" style="padding-top:6px;margin-bottom: 0"> 
		<div class="col-sm-12 ysbtn t_c">
			 <button type="button" class="btn btn-danger" delmore="program_gantt_workdel" from="program_gantt_worklist" style="  margin: 10px 9px;">删除</button>
			 <button type="button" class="btn btn-success" style="  margin: 10px 9px;" comadd="program_gantt_worklist" hcodeid="{{store('hcodeid')}}">添加行</button>
			 <button type="button" class="btn btn-blue" style="margin: 10px 9px;"><a href="hdt.html" target="_blank" style="color: #fff">打印预览</a></button><!-- preview -->
			 <button type="button" class="b_2 btn btn-default layui-layer-close" style="  margin: 10px 9px;">关闭</button>
		</div>
	</div>
	</div>
</div>
