  }
  .nav>li>a{
    color: #000;
    transition: .3s;
  }
  .nav>li:hover a{
    color: #32AAFD;
  }
    .nav>li>a.col:hover {
    color: #32AAFD;
}
  li.login>a{
    color: #6E6E6E;
