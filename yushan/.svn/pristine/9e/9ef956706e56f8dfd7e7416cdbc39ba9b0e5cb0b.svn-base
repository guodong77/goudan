header.m-hd nav .am-nav > li:hover a{ background-color:transparent;border-bottom:4px #425bad solid;}


header.paging-hd{ background-color:#fff; position:relative; z-index:80;-webkit-box-shadow:0 0 20px rgba(0, 0, 0, .2);  -moz-box-shadow:0 0 20px rgba(0, 0, 0, .2);  box-shadow:0 0 20px rgba(0, 0, 0, .2); }
header.paging-hd nav .am-nav > li > a{ color:#333;}
header.paging-hd nav .am-nav > li.am-active > a{color:#333;}

