header.m-hd nav .am-nav > li{padding:0 12px;position:relative;}
header.m-hd nav .am-nav > li.am-active{}
header.m-hd nav .am-nav > li > a{ padding:0 14px;font-size:18px; color:#fff;height:52px; line-height:48px;}
.am-nav > li.am-active > a, .am-nav > li.am-active > a:hover, .am-nav > li.am-active > a:focus{ background-color:transparent; border-bottom:4px #425bad solid;}
