.paging-checkbox{overflow:hidden;cursor:pointer;position: relative;line-height:30px; padding-left:30px;display:inline-block; width:100%;}
.paging-checkbox label{width:100%; cursor:pointer; display:inline-block; float:left;overflow:hidden;font-weight:500; color:#999999;}
.paging-checkbox input{ float:left; position:absolute; top:0; left:-20px;}
.paging-checkbox input[type="checkbox"]:checked + label:before,
.paging-checkbox input[type="checkbox"]:not(:checked) + label:before {content: " ";height:30px;position: absolute; left:0; top:0;width:30px;background:url(../images/d10.png) 10px center no-repeat;}
.paging-checkbox input[type="checkbox"]:checked + label:before {background:url(../images/d10s.png) 10px center no-repeat;}
.paging-checkbox input[type="checkbox"][disabled]:not(:checked) + label:before {background:url(../images/d10s.png) 10px center no-repeat;}





