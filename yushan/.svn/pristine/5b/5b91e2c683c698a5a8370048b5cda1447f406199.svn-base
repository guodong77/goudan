.common-ad{ width:100%; overflow:hidden; height:170px; background:url(../images/ac3.jpg) center  bottom no-repeat;}
.common-ad .box{ max-width:1200px; width:100%; overflow:hidden;margin:0 auto;height:170px;position:relative; }
