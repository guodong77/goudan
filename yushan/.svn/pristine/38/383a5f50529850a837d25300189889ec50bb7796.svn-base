    *{
        margin:0;
        padding: 0;
    }
    .fl{
        float: left;
    }
    .fr{
        float: right;
    }
    body{
        background: #f0f0f0;
    }
    #header{ 
        position: fixed;
        top:0;
        width:100%;
        height:40px;
        background: #29ABE2;
        line-height: 40px;
        color: #fff;
        font-size: 18px;
        z-index: 10;
    }
    #nav{
        position: fixed;
        top: 40px;
        min-width: 1200px;
        width:100%;
        height:40px;
        background: #FFFFFF;
        box-shadow: 0 1px 0 0 #E5E5E5;
        text-align: center;
        z-index: 10;
    }
    #nav ul li>a{
        color: #666;
    }
    #nav ul li:hover a{
        color: #29ABE2;
    }
    .customer{
        position: fixed;
        top: 50%;
        right:0;
        margin-top: -50px;
        width:40px;
        height:130px;
        z-index: 999;
        background: #29ABE2;
        padding:10px 5px;
        border-radius: 5px 0 0 5px;
        cursor: pointer;
    }
    .customer:hover{
        background:#29B6F2;
    }

    .h-conter{
        width: 1500px;
        margin:auto;
    }
    .border{
        position: absolute;
        border: 1px solid #29ABE2;
        width:75px;
        top: 38px;
        left: 36px;
        transition: .5s;
    }
    #nav ul li>a.selected{
        color: #29ABE2;
    }
    .content{
        width:1200px;
        margin:auto;
        background-color: #fff;
        border: 0;
        text-align:center;
    }
    .header1{
        display: flex;
        line-height: 42px;
        justify-content:space-between;
        border-bottom: 1px solid #e8e8e8;
        padding: 10px 15px;
        color: #29ABE2;
        font-size: 20px;
        box-shadow: 0 1px 0 0 #E5E5E5;
        opacity: 0;
    }
    .con1{
        display: none;
    }
    .text {
        border: 1px solid #29ABE2;
        padding: 5px 20px;
        border-radius: 4px;
        line-height: 30px;
    }
    .title{
        text-align: center;
        font-weight: bold;
        font-size: 24px;
        color: #333;
        border-bottom: 1px solid #e8e8e8;
        padding:30px 0;
    }
    .title1{
       background: #fff;
       border:0;
       font-size:20px;
       padding-left: 15px;
       padding-right: 15px;
       padding-bottom: 15px;
       font-weight: normal; 
    }
    .pre{
        background: #fff;
        padding: 20px 55px;
        border:0;
        font-size: 18px;
        color: #666666;
    }
    .pre1{
        padding-left: 15px;
        padding-right: 15px;
        padding-bottom: 0px;
    }
    .img{
       float: right;
       margin-right: 30px;
     }
     .weix{
        padding-top: 0;
     }
     .f14{
        font-size: 14px;
     }
     .color{
        color: #29ABE2;
     }
         @media screen and (max-width:1500px){
     .text1{
        margin-left: 50px;
     }
     .img{
       float: right;
       margin-right: 30px;
     }
     .h-conter{
        width:100%;
     }
    }
    @media screen and (max-width:1200px){
     .wrap {
       margin-left: 30px;
       margin-right: 30px;
      }
     .text1{
        margin-left: 50px;
     }
     .img{
       float: right;
       margin-right: 30px;
     }
     .h-conter{
        width:100%;
     }
    }
    @media screen and (max-width:990px){
        #header{
            display: none;
        }
        .content{
            display: block;
            width:80%;
            margin: auto;
        }
        h2{
            padding:0!important;
        }

        body{
            background: #fff;
        }
        .header1{
            opacity: 1;
        }
        .con1{
            display: none;
            padding: 0 30px;
        }
    }