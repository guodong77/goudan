<div class="h_p100 of_h pb_74 ps_r biaoge" ng-controller="com_ctrl">
 <div class="h_34" >

                <a delmore="system_tongbiao_del" from="system_tongbiao_list" class="btn btn-danger btn-xs delete"> <i class="fa fa-trash-o"></i>
                    删除
                </a>
                <a comadd="system_tongbiao_list" class="btn btn-blue btn-xs delete">添加</a>
              

        </div>


<table class="table table-striped table-bordered table-hover">
                    <thead class="bordered-palegreen">
                        <tr align="center">  
                        <th width="50" scope="col" style="padding: 6px;">
                    <label>
                        <input type="checkbox" comcheckall="system_tongbiao_list" class="colored-blue">
                        <span class="text"></span>
                    </label>
                </th>   
               <th width="8%">排序</th>
              <th>省统表名称</th>
              <th width="8%">省统表编码</th>
              <th width="8%"></th>
              <th width="8%">是否显示</th> 
             <!--  <th>统表类型</th>    -->       
                        </tr>

                    </thead>
               <tbody>
              <tr ng-if="value.isdel!=1"  align="center" ng-repeat="(key, value) in system_tongbiao_list.datalist"> 
              
              <td  align="center">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" class="colored-blue" comcheck="check" ng-checked="value.check==1">
                            <span class="text"></span>
                        </label>
                    </div>

                </td>
              <td width="8%"><input type="text" comedit="torder" value="{{value.torder}}" class="form-control"></td>
              <td><input type="text" comedit="tbname" value="{{value.tbname}}" class="form-control"></td>
               <td ><input type="text" comedit="code" value="{{value.code}}" class="form-control"></td>
               
 <td width="8%"> 
<div class="checkbox">
        <label>
          <input type="checkbox" checkedit="isshow" ng-checked="value.isshow==1">  
          <span class="text ng-binding"></span>
        </label>
      </div>
  </td> 
               <td width="8%">

 <select style="width:100%;"  comedit="tpid">
 <option value="0">无</option> 
                     <option ng-if="val.isfenbu!=1" value="{{val.tpid}}" ng-repeat="(k, val) in system_tongbiao_tplist.datalist" ng-selected="value.tpid==val.tpid">{{val.tpname}}</option> 
                    </select>
               </td>
              <!--  <td>
                 <input type="text" comedit="ftype" value="{{value.ftype}}" class="form-control">
               </td> -->
              
              
         	 </tr>
			</tbody>
 </table>

                <div id='page' class="t_c mt_10"></div>

</div>

